<div id="user_nav">
        <% if user_signed_in? %>
          Signed in as <%= current_user.email %>. Not you?
          <%= link_to "Sign out", logout_path, :method => :delete %>  	
        <% else %>
          <%= link_to "Sign up", register_path %> or 
          <%= link_to "Sign in", login_path %>    	
        <% end %>  
      </div>